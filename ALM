Sub SetFieldApp( FieldName, Vis, Req, PNo, VOrder )
  'Sets a field's appearance
  With Bug_Fields(FieldName)
    .IsVisible = Vis
    .IsRequired = Req
    .PageNo = PNo
    .ViewOrder = VOrder
  End With
End Sub



































































































































































































































































