Sub Bug_MoveTo

' Call DetectIdleUser

' below code added by venkat on 23/02/2025: auto update of Assigned Team

IF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleTester") Then
Bug_Fields("BG_USER_05").Value = "Test Team"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleTestManager") Then
Bug_Fields("BG_USER_05").Value = "Test Team"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleDeveloperSFCC") Then
Bug_Fields("BG_USER_05").Value = "SFCC"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleTesterBusiness") Then
Bug_Fields("BG_USER_05").Value = "Business HR"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleDeveloperIntegration") Then
Bug_Fields("BG_USER_05").Value = "Integration"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleTesterReleaseManagement") Then
Bug_Fields("BG_USER_05").Value = "Release Management"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleTesterPWC") Then
Bug_Fields("BG_USER_05").Value = "PWC"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleDeveloperData") Then
Bug_Fields("BG_USER_05").Value = "Data Team"
ElseIF isInGroup(Bug_Fields("BG_RESPONSIBLE").Value, "OnePeopleDeveloperSNOW") Then
Bug_Fields("BG_USER_05").Value = "SNOW"
Else    Bug_Fields("BG_USER_05").Value = "Triage"
End If

' above code added by Venkat.

' below code added by venkat on 14/04/2025

If Bug_Fields("BG_STATUS").Value <> "New" And _
   Bug_Fields("BG_STATUS").Value <> "Closed" And _
   Bug_Fields("BG_STATUS").Value <> "Rejected - Closed" And _
   Bug_Fields("BG_STATUS").Value <> "Duplicate - Closed" Then
    Bug_Fields("BG_USER_38").IsRequired = True
End If



