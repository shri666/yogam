' below code added by Venkat on 29/04/2025

    Dim currentDate, closingDate, threeDaysAgo

    ' Get the current date
    currentDate = Date()

    ' Calculate the date for 3 days ago
    threeDaysAgo = DateAdd("d", -3, currentDate)

    ' Get the defect's closing date
    closingDate = Bug_Fields("BG_CLOSING_DATE").Value


' Check if the closing date is within the last 3 days or is blank (open defect)
If IsNull(closingDate) Or closingDate = "" Or (closingDate >= threeDaysAgo And closingDate <= currentDate) Then

' Check if the field being updated is "Defect Category" and defect ID is 121
  If Bug_Fields("BG_BUG_ID").Value = 121 And FieldName = "BG_USER_08" Then
    ' Connect to ALM
    Set tdc = TDConnection ' Use the existing connection object

    ' Get the Defects Factory
    Set defectFactory = tdc.BugFactory
    Set defectList = defectFactory.NewList("")

    ' Loop through all defects
    For Each defect In defectList
        ' Calculate the defect age
        Dim creationDate, closeDate, defectAge
        creationDate = defect.Field("BG_DETECTION_DATE")

        ' Check for closed statuses
        If defect.Status = "Closed" Or defect.Status = "Rejected - Closed" Or defect.Status = "Duplicate - Closed" Then
            closeDate = defect.Field("BG_CLOSING_DATE")
            defectAge = DateDiff("d", creationDate, closeDate)
        Else
            defectAge = DateDiff("d", creationDate, Now)
        End If

        ' Update the custom field
        defect.Field("BG_USER_36") = defectAge
        defect.Post
    Next
    End If
End If




