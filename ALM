Sub WizardListCust
' This subroutine had been automatically generated by "Script Generator - List Customization".
' Any modification you make will be overwritten next time you use the generator.

    ' BEGIN: When primary Status value is changed, Status's selection list will change.
    Select Case Bug_Fields("BG_STATUS").Value
        Case "Closed"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_All_Statuses")
        Case "Deferred"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Deferred")
        Case "Duplicate"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Duplicate")
        Case "Duplicate - Closed"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Duplicate_Closed")
        Case "Fixed"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Fixed")
        Case "New"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_New")
        Case "Open"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Open")
        Case "Passed Retest"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Passed_Retest")
        Case "Ready for Test"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Ready_for_Test")
        Case "Rejected - Closed"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Rejected_Closed")
        Case "Rejected - Requested"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Rejected_Requested")
        Case "Reopen"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_Reopen")
        Case "More Info"
            Bug_Fields("BG_STATUS").List = Lists("Bug_Status_More_Info")
        Case Else
            Bug_Fields("BG_STATUS").List = Lists("Bug Status")
    End Select
    ' END: When primary Status value is changed, Status's selection list will change.

End Sub





